<?xml version="1.0"?>
<hibernate-mapping package="org.runbpm.entity">

    <class name="ProcessModelImpl" table="RUNBPM_PROCESS_MODEL">
        <id name="id" type="long">
            <generator class="assigned"/>
        </id>
        
        <property name="version" type="integer"/>
        
        <property name="name" type="string"/>
        
        <property name="createDate" type="timestamp"/>
        
        <property name="modifyDate" type="timestamp"/>
        
        <property name="processDefinitionId" type="string"/>
        
        <property name="xmlContent" type="materialized_clob"/>
        
    </class>

</hibernate-mapping>